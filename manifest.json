{
  "manifest_version": 3,
  "name": "Auto Schedule Register",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "identity",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://keep.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "options.html",
  "oauth2": {
    "client_id": "914778911874-qqr6ed7a4seco5gv3undh084cuob8som.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  }
}